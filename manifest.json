{
  "name": "Turtle Dialer",
  "version": "1.1.2",
  "manifest_version": 2,
  "description": "Control your IP phone from Google Chrome",
  "homepage_url": "http://www.turtledialer.com",
  "key": "",
  "icons": {
    "16": "icons/icon_16.png",
    "19": "icons/icon_19.png",
    "38": "icons/icon_38.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "omnibox": { "keyword" : "td" },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+D",
        "mac": "MacCtrl+Shift+D"
      },
      "description": "Open Turtle Dialer"
    }
  },
  "default_locale": "es",
  "background": {
    "scripts": [
      "js/jquery-1.11.1.min.js",
      "js/phoneformat.js",
      "js/papaparse.min.js",
      "js/md5.js",
      "js/config.js",
      "js/phones.js",
      "js/gcontacts.js",
      "js/numberutils.js",
      "js/notifications.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "options_page": "html/settings.html",
  "browser_action": {
    "default_icon": "icons/icon_38.png",
    "default_title": "Turtle dialer",
    "default_popup": "html/phonebook.html"
  },
  "permissions": [
    "tabs",
    "notifications",
    "storage",
    "contextMenus",
    "identity",
    "http://*/*",
    "https://*/*"
   ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file:///*/*"],
      "js": ["js/content.js"]
    }
  ],
  "oauth2": {
    "client_id": "101086173746-9l2ttrpkq7djht9232u1rm19i7t77pjm.apps.googleusercontent.com",
    "scopes": [
      "https://www.google.com/m8/feeds"
    ]
  }
}